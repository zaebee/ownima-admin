{
  "name": "ownima-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:development": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:beta": "vite --mode beta",
    "build": "tsc -b tsconfig.app.json && vite build",
    "build:development": "tsc -b tsconfig.app.json && vite build --mode development",
    "build:staging": "tsc -b tsconfig.app.json && vite build --mode staging",
    "build:beta": "tsc -b tsconfig.app.json && vite build --mode beta",
    "build:production": "tsc -b tsconfig.app.json && vite build --mode production",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "generate-types": "bunx openapi-typescript https://beta.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:development": "bunx openapi-typescript http://localhost:8080/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:staging": "bunx openapi-typescript https://stage.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:production": "bunx openapi-typescript https://api.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tanstack/react-query": "^5.90.21",
    "autoprefixer": "^10.4.24",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/trusted-types": "^2.0.7",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "lefthook": "^2.1.1",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.10",
    "openapi-typescript": "^7.13.0",
    "prettier": "^3.8.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.55.0",
    "undici": "^7.22.0",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
