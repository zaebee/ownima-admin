{
  "name": "ownima-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-types": "bunx openapi-typescript https://beta.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:local": "bunx openapi-typescript http://localhost:8000/api/v1/openapi.json -o src/types/api-generated.ts",
    "sacred:scan": "python ../../tools/agro_scanner.py $(find src -name '*.tsx' -o -name '*.ts' -o -name '*.vue' -o -name '*.js')",
    "sacred:self-check": "python ../../tools/agro_scanner.py --self-check",
    "sacred:validate": "npm run lint && npm run sacred:scan && tsc --noEmit",
    "sacred:build": "npm run sacred:validate && npm run build",
    "blessed:build": "npm run sacred:build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.87.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/trusted-types": "^2.0.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
