{
  "name": "ownima-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:development": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:beta": "vite --mode beta",
    "build": "tsc -b && vite build",
    "build:development": "tsc -b && vite build --mode development",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:beta": "tsc -b && vite build --mode beta",
    "build:production": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-types": "bunx openapi-typescript https://beta.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:development": "bunx openapi-typescript http://localhost:8080/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:staging": "bunx openapi-typescript https://stage.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts",
    "generate-types:production": "bunx openapi-typescript https://api.ownima.com/api/v1/openapi.json -o src/types/api-generated.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.87.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/trusted-types": "^2.0.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
